	<select id="EgovSellerDAO.selectLoginCheck1" parameterClass="SellerVO" resultClass="java.lang.String">
	<!-- 쿼리문 작성 -->
		<![CDATA[  
			SELECT S_ID
			FROM SELLER
			WHERE 1=1
			AND S_ID = #s_id#
			AND S_PASS = #s_pass#
		]]>
	</select>
