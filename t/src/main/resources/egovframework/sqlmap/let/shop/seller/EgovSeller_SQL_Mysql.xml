	<select id="EgovMngMemberDAO.selectMngMemberList" parameterClass="MemberVO" resultMap="mngMemberList" >
		<![CDATA[
			SELECT 
				   M_ID
				  ,M_NICKNAME
				  ,PASSWORD
				  ,EMAIL
				  ,PHOTO
