	<update id="updateMngSellerStateChange" parameterClass="java.lang.String">
		UPDATE SELLER
		SET s_status = 
						CASE
						WHEN s_status = 'Y'
							THEN 'N'
							ELSE 'Y'
						END
		WHERE s_id = #s_id#
	</update>
